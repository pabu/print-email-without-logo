{
  "name": "Print email without logo",
  "version": "0.0.5",
  "manifest_version": 2,
  "description": "Print email content without email service provider logo for Gmailâ„¢.",
  "homepage_url": "https://github.com/zhangsu/print-email-without-logo",
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/mail/*&view=pt*"],
      "js": ["js/hide_logo.js"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    "css/hide_logo.css"
  ]
}
